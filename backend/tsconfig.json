{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types",
      "./type.d.ts"
    ],
  },
    "include": ["./server.ts", "./models/**/*.ts", "./routes/**/*.ts"],
    "exclude": ["node_modules"]
}